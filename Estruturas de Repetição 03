Var
   cont, maior, N, S, menor : inteiro
Inicio
cont <- 1
S <- 0
enquanto (cont <= 5) faca
         escreva("digite o", cont, "°. valor:")
         leia (N)
         se ( N > maior) entao
         maior <- N
         fimse
         se (menor >= 0) entao
       menor <- N
fimse
         S <- S + N
         cont <- cont + 1
         fimenquanto
              escreval(" a soma de todos os valores foi: ", S)
              ESCREVAL(" o maior número digitado foi: ", maior )
              escreval(" já o menor número digitado foi:", menor)
              


Fimalgoritmo
